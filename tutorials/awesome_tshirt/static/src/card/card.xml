<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-name="awesome_tshirt.card" owl="1">
        <div class="card m-1" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title fw-bolder"><t t-esc="props.key"/></h5>
                <p class="card-text">
                    <t t-esc="props.value"/>
                </p>
            </div>
        </div>
    </t>
</templates>